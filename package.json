{
	"name": "zaki",
	"displayName": "Zaki",
	"description": "CEQUENS Autonomous coding agent right in your IDE, capable of creating/editing files, running commands, using the browser, and more with your permission every step of the way.",
	"version": "1.2.6",
	"icon": "assets/icons/ceq-logo.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "CEQUENS"
	},
	"license": "Apache-2.0",
	"publisher": "AIHub Squad",
	"repository": {
		"type": "git",
		"url": "https://github.com/CEQUENS/Zaki"
	},
	"homepage": "https://Zaki.bot",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"Zaki",
		"claude",
		"dev",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "claude-dev-ActivityBar",
					"title": "Zaki",
					"icon": "$(robot)"
				}
			]
		},
		"views": {
			"claude-dev-ActivityBar": [
				{
					"type": "webview",
					"id": "claude-dev.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "zaki.checkForExtensionUpdate",
				"title": "zaki: Check for new extension version..."
			},
			{
				"command": "zaki.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "zaki.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "zaki.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "zaki.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "zaki.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "zaki.openInNewTab",
				"title": "Open In New Tab",
				"category": "Zaki"
			},
			{
				"command": "zaki.checkForExtensionUpdate",
				"title": "Check for Extension Update",
				"category": "Zaki"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "zaki.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "zaki.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "zaki.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "zaki.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "zaki.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == claude-dev.SidebarProvider"
				}
			],
			"extension/context": [
				{
					"command": "zaki.checkForExtensionUpdate",
					"when": "extension == CEQ.zaki && extensionStatus == installed"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run build:webview && npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "jest",
		"test:watch": "jest --watch",
		"install:all": "npm install && cd webview-ui && npm install",
		"start:webview": "cd webview-ui && npm run start",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish && ovsx publish"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/diff": "^5.2.1",
		"@types/jest": "^29.5.14",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/tmp": "^0.2.6",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"esbuild": "^0.21.5",
		"eslint": "^8.57.0",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.32.1",
		"@anthropic-ai/vertex-sdk": "^0.4.1",
		"@google/generative-ai": "^0.18.0",
		"@modelcontextprotocol/sdk": "^1.0.1",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"debounce": "^2.2.0",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"fast-deep-equal": "^3.1.3",
		"globby": "^14.0.2",
		"isbinaryfile": "^5.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"openai": "^4.61.0",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"strip-ansi": "^7.1.0",
		"tmp": "^0.2.3",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.23.8"
	}
}
